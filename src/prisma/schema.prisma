generator client {
  provider = "prisma-client-js"
}

generator dbml {
  provider            = "prisma-dbml-generator"
  output              = "./"
  outputName          = "schema.dbml"
  projectDatabaseType = "MySQL"
  projectName         = "Airbnb Clone"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
